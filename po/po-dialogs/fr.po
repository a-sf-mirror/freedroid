msgstr "Cette arène a déjà débuté."

#: dialogs/Mike.lua:75
msgid "Hi."
msgstr "Salut."

#: dialogs/Mike.lua:78
msgid "Salutations: Welcome to the arena."
msgstr "Salutations : Bienvenue dans l'arène."

#: dialogs/Mike.lua:79
msgid "Salutations: M.I.K.E. says hello."
msgstr "Salutations : M.I.K.E. dit bonjour."

#: dialogs/Mike.lua:82
msgid "Salutations: Greeting, Fighter."
msgstr "Salutations : Bienvenu, Combattant."

#: dialogs/Mike.lua:94
msgid "I want to fight."
msgstr "Je veux combattre."

#: dialogs/Mike.lua:97
msgid "Excellent: New challengers are greatly appreciated."
msgstr "Excellent : Les nouveaux challenges sont grandement appréciés."

#: dialogs/Mike.lua:98
msgid "Registration: Patience, it will take a litte moment."
msgstr "Enregistrement : Patience, cela va prendre un petit moment."

#: dialogs/Mike.lua:99
msgid "[b]...Analysis of the bio-signature.[/b]"
msgstr "[b]...Analyse de la bio-signature.[/b]"

#: dialogs/Mike.lua:100
msgid "[b]...Processing the ranking value.[/b]"
msgstr "[b]...Calcul de la place dans le classement.[/b]"

#: dialogs/Mike.lua:101
msgid "[b]...Saving personal characteristics.[/b]"
msgstr "[b]...Sauvegarde des caractéristiques personnelles.[b]"

#: dialogs/Mike.lua:103
msgid "[b]...Fighter registered.[/b]"
msgstr "[b]...Combattant enregistré.[/b]"

