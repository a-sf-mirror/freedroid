#: data/storyline/act1/dialogs/HF-EntranceBot.lua:71
msgid "[b]Validating certificate...[/b]"
msgstr "[b]Validazione certificato...[/b]"

#: data/storyline/act1/dialogs/HF-EntranceBot.lua:36
#: data/storyline/act1/dialogs/HF-EntranceBot.lua:72
